# Environment Variables Template
# Copy this file to .env.local and fill in your values

# Application
NEXT_PUBLIC_APP_NAME="OSNEPAL"
NEXT_PUBLIC_APP_DESCRIPTION="Open Source Nepal - Building the future together"
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# API Configuration
NEXT_PUBLIC_API_URL="https://api.osnepal.org"
NEXT_PUBLIC_API_VERSION="v1"

# Analytics (Optional)
NEXT_PUBLIC_GA_ID=""
NEXT_PUBLIC_GTM_ID=""

# Social Media Links
NEXT_PUBLIC_GITHUB_URL="https://github.com/osnepal"
NEXT_PUBLIC_DISCORD_URL="https://discord.gg/GQGFKDAQ9b"
NEXT_PUBLIC_TWITTER_URL=""
NEXT_PUBLIC_LINKEDIN_URL=""

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS="false"
NEXT_PUBLIC_ENABLE_DEBUG="true"

# External Services (Optional)
NEXT_PUBLIC_SENTRY_DSN=""
NEXT_PUBLIC_HOTJAR_ID=""

# Development
NODE_ENV="development"
